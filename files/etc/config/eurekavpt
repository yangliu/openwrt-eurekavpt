	option mode 'isp'
	option hijacklandns '0'
	option gfwlist '1'
	list whitelist_ip ''
	list blacklist_ip ''
	list whitelist_domain ''
	list blacklist_domain ''
	option ingfw_dns 'isp'
	option ingfw_dns_port '53'
	option d2s_local_ip '127.0.0.1'
